{
  "expo": {
    "name": "GreenTech",
    "slug": "greentech",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "greentech",
    "splash": {
      "backgroundColor": "#16A34A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.greentech.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#16A34A"
      },
      "package": "com.greentech.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store", "expo-image-picker"]
  }
}
